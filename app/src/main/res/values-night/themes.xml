<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.FippApp" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/verdePrincipal</item>
        <item name="colorPrimaryVariant">@color/verdeSecundario</item>
        <item name="colorOnPrimary">@color/grisOscuro</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/moradoPrincipal</item>
        <item name="colorSecondaryVariant">@color/moradoSecundario</item>
        <item name="colorOnSecondary">@color/blanco</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>