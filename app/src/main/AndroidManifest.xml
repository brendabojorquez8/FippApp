<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bojorquez.brenda.fippapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/fippapp_iconapp_01"
        android:label="@string/app_name"
        android:roundIcon="@drawable/fippapp_iconapp_01"
        android:supportsRtl="true"
        android:theme="@style/Theme.FippApp">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CrearCuenta"/>
        <activity android:name=".Login"/>
        <activity android:name=".Home"/>
        <activity android:name=".BalanceIngresos" />
        <activity android:name=".BalanceGastos" />
        <activity android:name=".RegistrarIngreso" />
        <activity android:name=".RegistrarGasto" />
        <activity android:name=".CategoriasGastos" />
        <activity android:name=".CategoriasIngresos" />
        <activity android:name=".ClasificacionGastos" />
        <activity android:name=".ClasificacionIngresos" />
        <activity android:name=".GastosFijos" />
        <activity android:name=".IngresosFijos" />
        <activity android:name=".RegistrarCategoriasIngresos" />
        <activity android:name=".RegistrarCategoriasGastos" />
    </application>

</manifest>